{
  "name": "blahblah-app",
  "version": "0.1.0",
  "scripts": {
    "clean": "rimraf eslint build .cache coverage *.xml *.log",
    "lint": "eslint . --ignore-path .eslintignore",
    "lint:html": "eslint . --ignore-path .eslintignore -f html -o eslint/index.html",
    "lint:checkstyle": "eslint . --ignore-path .eslintignore -f checkstyle > checkstyle-result.xml",
    "lint:report": "yarn clean && yarn lint:html && yarn lint:checkstyle",
    "sass": "gulp sass",
    "gulp": "gulp",
    "start": "gulp & react-scripts start",
    "build": "npm run sass && react-scripts build",
    "test": "jest --env=jsdom",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "blahblah-integration": "^0.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blah-blah-tech/blahblah.tech.git"
  },
  "author": "stunstunstun",
  "license": "MIT"
}
