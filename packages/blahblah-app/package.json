{
  "name": "blahblah-app",
  "version": "0.1.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "watch:css": "gulp",
    "build:css": "gulp sass",
    "build:js": "react-scripts build",
    "build:client": "react-scripts build",
    "build:server": "babel index.js -d build --presets env",
    "build": "cross-env NODE_ENV=production run-s build:css build:client build:server",
    "start:server": "node build/index.js",
    "start:client": "react-scripts start",
    "start": "npm-run-all -p watch:css start:client start:server",
    "serve": "pm2 start build/index.js -i 4 --name blahblah --merge-logs --log-date-format='YYYY-MM-DD HH:mm Z' && pm2 save 2> /dev/null",
    "prepublish": "cross-env NODE_ENV=production yarn build"
  },
  "dependencies": {
    "blahblah-integration": "^1.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "homepage": "http://www.blahblah.tech",
  "repository": {
    "type": "git",
    "url": "https://github.com/blah-blah-tech/blahblah.tech.git"
  },
  "author": "stunstunstun",
  "license": "MIT"
}
