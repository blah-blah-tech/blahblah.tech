{
  "name": "@githubjobs/server",
  "version": "1.0.0",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "lint": "eslint '__tests__/**' 'src/**'",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "coverage": "yarn test --coverage",
    "pm2": "pm2",
    "start": "NODE_ENV=development NODE_PATH=./ nodemon src",
    "serve": "NODE_ENV=production NODE_PATH=./ PORT=10080 pm2 start ./src --name '@githubjobs/server' && pm2 save 2> /dev/null"
  },
  "dependencies": {
    "@githubjobs/domain": "^1.0.0",
    "@githubjobs/integration": "^1.0.0",
    "apollo-server": "^2.0.7",
    "apollo-server-koa": "2.0.4",
    "body-parser-graphql": "^1.1.0",
    "dataloader": "^1.4.0",
    "graphql": "^14.0.2",
    "graphql-compose": "^5.2.0",
    "graphql-compose-connection": "^4.0.0",
    "graphql-compose-mongoose": "^5.1.0",
    "graphql-compose-pagination": "^4.0.0",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-jwt": "^3.5.1",
    "koa-morgan": "^1.0.1",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.3",
    "pm2": "^3.2.2",
    "supertest": "^3.3.0"
  },
