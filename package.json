{
  "name": "blahblah.tech",
  "private": true,
  "version": "0.1.0",
  "description": "We can do blahblah about the Programming",
  "scripts": {
    "clean": "lerna run clean",
    "lint": "lerna run lint",
    "lint:report": "lerna run lint:report",
    "test": "jest",
    "coverage": "yarn test --no-cache --runInBand --coverage",
    "coverage:report": "yarn coverage && codecov",
    "build": "lerna run build --parallel",
    "build:integration": "lerna exec --scope blahblah-integration yarn build",
    "start": "lerna exec --scope blahblah-app yarn start",
    "format": "prettier --write 'packages/**/*.js'",
    "precommit": "lerna run precommit"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-sass": "^3.1.0",
    "husky": "^0.14.3",
    "lerna": "^2.9.0",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "react-scripts": "^1.1.1",
    "rimraf": "^2.6.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=8.9.4"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blah-blah-tech/blahblah.tech.git"
  },
  "homepage": "http://www.blahblah.tech",
  "author": "stunstunstun",
  "license": "MIT"
}
